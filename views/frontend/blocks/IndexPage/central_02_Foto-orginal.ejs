<div class="templatemo-team" id="templatemo-about">
            <div class="container">
                <div class="row">
                    
                        <div class="text-center">
                            <h3>Должностные лица товариществ</h3>
                            
                        </div>
                    
                </div>
                <div class="clearfix"> </div>
				<% if(foto.length > 0){ %>
                    <ul class="row row_team">
                        <% foto.forEach(function(detail,i){%>
						<% if(i < 40){%> <!-- output only 4 face -->
						<li class="col-lg-3 col-md-3 col-sm-6 ">
                            <div class="text-center">
                                <div class="member-thumb">
                                    <% if(detail.foto){ %>
										<img src="images/face/<%= detail.foto %>" class="img-responsive" width="500" height="" alt="" />
									<% }else{ %>
										<img src="images/face/nofoto.jpg" class="img-responsive" width="500" height="500" alt="нет фото" />
									<% } %>
                                    <!--<div class="thumb-overlay"></div>-->
                                </div>
                                <div class="team-inner">
                                    <p class="team-inner-header"><%= detail.fio %></p>
                                    <p class="team-inner-subtext"><%= detail.phone %></p>
									<p class="team-inner-subtext"><%= detail.dolz %></p>
									
                                </div>
                            </div>
                        </li>
						<% } %>
						<% }) %>
                    </ul>
				<% } %>
            </div>
</div>