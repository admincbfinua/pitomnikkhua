<div class="templatemo-team" id="templatemo-about">
            <div class="container">
                <div class="row">
                    
                        <div class="text-center">
                            <h3>Должностные лица товариществ</h3>
                            
                        </div>
                    
                </div>
                <div class="clearfix"> </div>
				<% var flenght = foto.length %>
				
				<% if(flenght > 1 && flenght < 12){ %>
                  
                        <% foto.forEach(function(detail,i){%>
						<% if(i == 0 || i == 6){%> 
						<div class="row row_team">
						<% } %>
						<div class="col-lg-2 col-md-2 col-sm-6 ">
                            <!--<div class="text-center">-->
                                <div class="member-thumb">
                                    <% if(detail.foto){ %>
										<img src="images/face/<%= detail.foto %>" class="img-responsive" width="150" height="" alt="" />
									<% }else{ %>
										<img src="images/face/nofoto.jpg" class="img-responsive" width="150" height="" alt="нет фото" />
									<% } %>
                                    <!--<div class="thumb-overlay"></div>-->
                                </div>
                                <div class="team-inner">
                                    <p class="team-inner-header"><%= detail.fio %></p>
                                    <p class="team-inner-subtext"><%= detail.phone %></p>
									<p class="team-inner-subtext"><%= detail.dolz %></p>
									
                                </div>
                            <!--</div>-->
                        </div>
						<% if(i == 5 || flenght-1 == i || i==11){%> 
						</div>
						<% } %>
						<% }) %>
						
                   
				<% } %>
            </div>
</div>