<% if(data){ %>
 <form class="form-horizontal" method="POST" name="editdachiupload" action="">
 
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Номер участка</label>
    <div class="col-sm-10">
      <input type="text" name="num_uc" value="<%= data[0].num_uc %>" class="form-control" placeholder="Номер участка">
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Наименование товарищества</label>
    <div class="col-sm-10">
      <select name="naim_tov" required><option><%= data[0].naim_tov %></option>
		<option value="50 лет Октября">50 лет Октября</option>
		<option value="Шахтер">Шахтер</option>
		<option value="Дзержинец">Дзержинец</option>
		
	</select>
	  
    </div>
  </div>
  
	<div class="form-group">
		<label class="col-sm-2 control-label">Описание участка(дачи)</label>
		<div class="col-sm-10"> 
			<textarea name="opis_uc" class="form-control" rows="5"><%= data[0].opis_uc %></textarea>
		</div>
	</div>
  
  <div class="form-group">
    <label class="col-sm-2 control-label">Общая площадь участка</label>
    <div class="col-sm-10">
      <input type="text" name="plosh_uc" value="<%= data[0].plosh_uc %>" class="form-control" placeholder="Общая площадь участка">
    </div>
  </div>
  
    
  <div class="form-group">
    <label class="col-sm-2 control-label">Желаемая стоимость(грн)</label>
    <div class="col-sm-10">
      <input type="text" name="price" value="<%= data[0].price %>" class="form-control" placeholder="Желаемая стоимость(грн)">
    </div>
  </div>
  
  <div class="form-group">
    <label class="col-sm-2 control-label">Телефон контактного лица</label>
    <div class="col-sm-10">
      <input type="text" name="contact_phone" value="<%= data[0].contact_phone %>" class="form-control" placeholder="Телефон контактного лица">
    </div>
  </div>
  <!--<div class="form-group">
    <label class="col-sm-4 control-label">Фото(CTRL+левая кнопка мышки - для множественного выбора )</label>
    <div class="col-sm-8">
      <input type="file" name="photo" multiple="multiple" class="">
    </div>
  </div>-->
  
   <div class="form-group">
    <label class="col-sm-2 control-label">Имя контактного лица</label>
    <div class="col-sm-10">
      <input type="text" name="contact_fio" value="<%= data[0].contact_fio %>" class="form-control" placeholder="Имя отчество(кого спрашивать)">
    </div>
  </div>
  
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <input type="submit" name="submit" value="Сохранить">
	 <input type="button" name="cancel" value="Отменить" class="cancel" onclick="canceldachiadd()">
    </div>
  </div>
</form>
<% } %>