 <h2 class="sub-header">Редактирование записи по решению собрания</h2>
 <% if(data){ %>
 <form class="form-horizontal" id="shablondocs_data" method="post" action="/backend/shablondocs/edit/<%= data[0].id %>">
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Повестка дня</label>
    <div class="col-sm-10">
      <input type="text" name="povestka" value="<%= data[0].povestka %>" class="form-control" placeholder="Повестка дня">
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Кто проводит</label>
    <div class="col-sm-10">
      <input type="text" name="ktoprovodit" value="<%= data[0].ktoprovodit %>" class="form-control" placeholder="Кто проводит">
    </div>
  </div>
 <div class="form-group">
    <label class="col-sm-2 control-label">Полное описание</label>
    <div class="col-sm-10">
      <textarea name="fulldescr" class="form-control" rows="5"><%= data[0].fulldescr %></textarea>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Решение</label>
    <div class="col-sm-10">
      <textarea name="reshenie" class="form-control" rows="5"><%= data[0].reshenie %></textarea>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <input type="submit" name="submit" value="Сохранить">
	 <input type="button" name="cancel" value="Cancel" class="cancel" onclick="cancelAdd()">
    </div>
  </div>
</form>
<% } %>		 
		 
		 
		 
		 
		 