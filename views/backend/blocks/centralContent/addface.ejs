 <h2 class="sub-header">Добавление информации по должностному лицу</h2>
 <form class="form-horizontal" method="POST" name="upload" enctype="multipart/form-data" action="">
 
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">ФИО</label>
    <div class="col-sm-10">
      <input type="text" name="fio" class="form-control" placeholder="ФИО">
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Должность</label>
    <div class="col-sm-10">
      <input type="text" name="dolz" class="form-control" placeholder="Должность">
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Телефон</label>
    <div class="col-sm-10">
      <input type="text" name="phone" class="form-control" placeholder="Телефон">
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Фото</label>
    <div class="col-sm-10">
      <input type="file" name="foto" multiple="multiple" class="form-control">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <input type="submit" name="submit" value="Сохранить">
	 <input type="button" name="cancel" value="Отменить" class="cancel" onclick="cancelFace()">
    </div>
  </div>
</form>
 
 

 